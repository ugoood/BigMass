package cn.wk.gui;

import java.awt.Button;
import java.awt.Frame;

public class FrameDemo {

	public static void main(String[] args) {
		Frame f = new Frame("我的窗体");
		f.setBounds(400, 150, 400, 300);
				
		Button but = new Button("退出");
		f.add(but);
		
		
		
		f.setVisible(true);
		System.out.println("over");

	}

}
